pipeline {
    agent any

    stages {
      	stage ('checkout repository'){
		  $class: 'GitSCM', branches: [[name: '*/master']],
		  userRemoteConfigs: [[url: 'https://github.com/BdanTehAwesome/JenkinsPipeline.git',credentialsId:'a8157e19-b483-4828-82dd-8f871d58be4e']]
 		}
			
        stage('Maven Compile') {
            steps {
                bat 'mvn compile'
            }
        }
        stage('Run Junit Tests') {
            steps {
                bat 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}