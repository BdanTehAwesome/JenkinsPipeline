pipeline {
    agent any

    stages {
      	stage ('checkout repository'){
	   	 	git(
	     	url: 'https://github.com/BdanTehAwesome/JenkinsPipeline.git',
	        credentialsId: 'a8157e19-b483-4828-82dd-8f871d58be4e',
	        branch: "master")
 		}
			
        stage('Maven Compile') {
            steps {
                bat 'mvn compile'
            }
        }
        stage('Run Junit Tests') {
            steps {
                bat 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}